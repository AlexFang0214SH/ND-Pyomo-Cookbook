    "from pyomo.environ import *\n",
    "from pyomo.dae import *\n",
    "\n",
    "N = 5\n",
    "tfinal = 500\n",
    "\n",
    "\n",
    "m = ConcreteModel()\n",
    "\n",
    "m.t = ContinuousSet(bounds=(0, tfinal))\n",
    "m.stages = Set(initialize=list(range(1,N+1)))\n",
    "\n",
    "m.c = Var(m.stages, m.t, domain=NonNegativeReals)\n",
    "m.s = Var(m.stages, m.t, domain=NonNegativeReals)\n",
    "\n",
    "\n"
   ]
